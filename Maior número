#include <stdio.h>
#include <locale.h>
#include <string.h>
#include <math.h>


int main(){
    setlocale(LC_ALL, "Portuguese");

    int v[10], i, maior=0;

    for(i=0;i<10;i++){
        printf("Digite um número: ");
        scanf("%d",&v[i]);
        if(v[i]>maior){
            maior=v[i];
        }
    }

    printf("\nMaior número: %d",maior);

    return 0;
}
