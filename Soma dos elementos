#include <stdio.h>
#include <locale.h>
#include <string.h>
#include <math.h>


int main(){
    setlocale(LC_ALL, "Portuguese");

    int v[10], i, soma=0;

    for(i=0;i<10;i++){
        printf("Digite um nÃºmero: ");
        scanf("%d",&v[i]);
        soma = soma+v[i];
    }

    printf("\nSoma: %d",soma);

    return 0;
}
